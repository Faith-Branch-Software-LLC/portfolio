{
  "name": "portfolio",
  "version": "1.2.0",
  "private": true,
  "scripts": {
    "wasm:build": "cd wasm/clipper-offset && wasm-pack build --target web --out-dir pkg",
    "wasm:build:release": "cd wasm/clipper-offset && wasm-pack build --target web --release --out-dir pkg",
    "wasm:copy": "mkdir -p public/wasm && cp wasm/clipper-offset/pkg/clipper_offset_bg.wasm wasm/clipper-offset/pkg/clipper_offset.js public/wasm/",
    "wasm:dev": "pnpm run wasm:build && pnpm run wasm:copy",
    "wasm:prod": "pnpm run wasm:build:release && pnpm run wasm:copy",
    "dev": "pnpm run wasm:dev && next dev --turbopack",
    "build": "pnpm run wasm:prod && next build",
    "start": "next start",
    "lint": "next lint",
    "generate": "prisma generate",
    "migrate": "prisma migrate dev",
    "studio": "prisma studio",
    "deploy:production": "dotenv -f '.env.production' kamal deploy -d production",
    "deploy:staging": "dotenv -f '.env.staging' kamal deploy -d staging"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.10.0",
    "@prisma/adapter-mariadb": "^7.0.0",
    "@prisma/client": "7",
    "@radix-ui/react-label": "^2.1.1",
    "@radix-ui/react-select": "^2.1.4",
    "@radix-ui/react-separator": "^1.1.1",
    "@radix-ui/react-slot": "^1.1.1",
    "@radix-ui/react-switch": "^1.1.2",
    "@radix-ui/react-toast": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.1.6",
    "@react-three/fiber": "^8.17.12",
    "@tailwindcss/typography": "^0.5.16",
    "@types/jsdom": "^21.1.7",
    "@types/three": "^0.166.0",
    "canvas-confetti": "^1.9.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dompurify": "^3.2.5",
    "dotenv": "^16.4.7",
    "fs": "0.0.1-security",
    "gray-matter": "^4.0.3",
    "gsap": "^3.14.2",
    "jsdom": "^24.0.0",
    "lucide-react": "^0.472.0",
    "mariadb": "^3.4.5",
    "marked": "^15.0.7",
    "motion": "^12.6.5",
    "next": "16.0.7",
    "page-flip": "^2.0.7",
    "prisma": "7",
    "react": "19.2.1",
    "react-dom": "19.2.1",
    "react-hook-form": "^7.54.2",
    "react-icons": "^5.4.0",
    "resend": "^4.1.1",
    "shiki": "^3.19.0",
    "tailwind-merge": "^2.6.0",
    "three": "^0.166.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/canvas-confetti": "^1.9.0",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^22.10.6",
    "@types/react": "19.0.7",
    "@types/react-dom": "19.0.3",
    "eslint": "^9.18.0",
    "eslint-config-next": "16.0.7",
    "postcss": "^8.5.1",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.3"
  },
  "pnpm": {
    "overrides": {
      "@types/react": "19.0.7",
      "@types/react-dom": "19.0.3"
    },
    "onlyBuiltDependencies": [
      "@prisma/client",
      "@prisma/engines",
      "esbuild",
      "prisma",
      "sharp",
      "unrs-resolver"
    ]
  }
}
