{
  "name": "portfolio",
  "version": "1.2.0",
  "private": true,
  "scripts": {
    "wasm:copy": "mkdir -p public/wasm && cp node_modules/gsap-offset-path/wasm/pkg/* public/wasm/",
    "dev": "pnpm wasm:copy && next dev --turbopack",
    "build": "pnpm wasm:copy && next build",
    "start": "next start",
    "lint": "next lint",
    "generate": "prisma generate",
    "migrate": "prisma migrate dev",
    "studio": "prisma studio",
    "deploy:production": "dotenv -f '.env.production' kamal deploy -d production",
    "deploy:staging": "dotenv -f '.env.staging' kamal deploy -d staging",
    "logs": "dotenv -f '.env.production' kamal app logs -f -d production",
    "compile:blog": "tsx scripts/compile-blog.ts",
    "seed": "tsx prisma/seed.ts"
  },
